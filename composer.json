{
    "name": "codeq/zoom-api",
    "description": "Gets upcoming Zoom meetings and Zoom recordings via Eel Helper",
    "type": "neos-package",
    "license": "GPL-3.0-or-later",
    "require": {
        "neos/neos":  "^7.0 || ^8.0 || dev-master",
        "guzzlehttp/guzzle": "^7.4",
        "php": "^8"
    },
    "autoload": {
        "psr-4": {
            "CodeQ\\ZoomApi\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeQ\\ZoomApi\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Roland Sch√ºtz",
            "email": "rs@codeq.at",
            "homepage": "https://codeq.at",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore"
        ]
    },
    "extra": {
        "neos": {
            "package-key": "CodeQ.ZoomApi"
        }
    },
    "config": {
        "allow-plugins": {
            "neos/composer-plugin": true
        },
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "@stable",
        "mikey179/vfsstream": "@stable"
    },
    "scripts": {
        "test": "Packages/Libraries/phpunit/phpunit/phpunit --bootstrap Tests/UnitTestBootstrap.php --configuration Tests/phpunit.xml"
    }
}
